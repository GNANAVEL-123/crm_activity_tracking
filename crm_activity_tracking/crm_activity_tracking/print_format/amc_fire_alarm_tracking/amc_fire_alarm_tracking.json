 "css": ".print-format{\n     line-height:1  !important;\n     /*margin-top:15px;*/\n     /*margin-bottom:55px !important;*/\n}\n.print-format td, .print-format th {\n\tpadding: 4px !important;\n\tfont-size:11px;\n}\n\n.main{\n    border:1px solid black;\n}\n.highlight {\n    color: red;\n    font-weight: bold;\n}\n.header {\n    font-weight: bold;\n}\n.sub-section {\n    text-align: left;\n}",
